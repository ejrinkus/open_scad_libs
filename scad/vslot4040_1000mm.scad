        [5.36, 12.1, 0.0],
        [12.1, 12.1, 0.0],
        [12.1, 5.36, 0.0],
        [15.2, 2.26, 0.0],
        [15.2, 0.0, 0.0]
    ];

    solid = linear_extrude(height = height, center = False, convexity = 10, twist = 0)(
        polygon(points)
