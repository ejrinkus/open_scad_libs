            )
        )
    );

    return solid;


def make_4040(height):
    extrusion = make_2020(height);
    extrusion += right(20)(
        make_2020(height)
    );
    extrusion += forward(20)(
        make_2020(height)
    );
    extrusion += forward(20)(
        right(20)(
            make_2020(height)
        )
    );

    extrusion -= forward(1.8)(
        right(1.8)(
            center_hole_4040(height)
        )
    );

    return extrusion;

